{
  "name": "kermite",
  "productName": "Kermite",
  "version": "0.1.0-alpha.30",
  "author": "yahiro",
  "description": "keyboard software",
  "license": "MIT",
  "engines": {
    "node": "16"
  },
  "private": true,
  "scripts": {
    "start0": "ts-node build.ts -x-watch -x-exec",
    "start0:dev": "cross-env NODE_ENV=development ts-node build.ts -x-watch -x-exec",
    "start": "vite",
    "build": "tsc && vite build",
    "build0": "cross-env NODE_ENV=production ts-node build.ts -x-build",
    "packaging:mac": "electron-builder --mac --x64",
    "packaging:win": "electron-builder --win --x64",
    "mockview": "cross-env NODE_ENV=development ts-node build2.ts -x-mockview",
    "web": "cross-env NODE_ENV=development ts-node build2.ts -x-web",
    "clean": "rm -rf dist .cache",
    "typecheck": "tsc",
    "typecheck:full": "tsc -b src/*",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "incver": "npm version patch",
    "test": "jest",
    "visor": "alumina-visor serve"
  },
  "dependencies": {
    "alumina": "^0.1.14",
    "immer": "^9.0.15",
    "path-browserify": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/w3c-web-hid": "^1.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "alumina-visor": "^0.1.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-standard": "^5.0.0",
    "estrella": "^1.4.1",
    "jest": "^28.1.1",
    "open": "^8.4.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "servor": "^4.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "vite": "^2.9.12",
    "vite-plugin-environment": "^1.1.1"
  },
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.17"
  }
}

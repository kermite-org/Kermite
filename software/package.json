{
  "name": "kermite",
  "productName": "Kermite",
  "version": "0.1.0-alpha.29",
  "main": "dist/shell/index.js",
  "author": "yahiro",
  "description": "keyboard software",
  "license": "MIT",
  "engines": {
    "node": "14"
  },
  "build": {
    "appId": "org.kermite.electron-app",
    "mac": {
      "target": "pkg",
      "icon": "misc/iconC.png"
    },
    "win": {
      "target": "nsis",
      "icon": "misc/iconC.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "buildDependenciesFromSource": true
  },
  "private": true,
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "ts-node build.ts -x-watch -x-exec",
    "start:dev": "cross-env NODE_ENV=development ts-node build.ts -x-watch -x-exec",
    "build": "cross-env NODE_ENV=production ts-node build.ts -x-build",
    "packaging:mac": "electron-builder --mac --x64",
    "packaging:win": "electron-builder --win --x64",
    "mockview": "cross-env NODE_ENV=development ts-node build2.ts -x-mockview",
    "web": "cross-env NODE_ENV=development ts-node build2.ts -x-web",
    "clean": "rm -rf dist .cache",
    "typecheck": "tsc",
    "typecheck:full": "tsc -b src/*",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "incver": "npm version patch",
    "test": "jest",
    "visor": "alumina-visor serve"
  },
  "dependencies": {
    "alumina": "^0.1.11",
    "glob": "^7.2.0",
    "immer": "^9.0.7",
    "node-fetch": "3.0.0",
    "node-hid": "^2.1.1",
    "serialport": "9.2.8",
    "usb": "^2.0.3"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/node-fetch": "^3.0.3",
    "@types/node-hid": "^1.3.1",
    "@types/serialport": "^8.0.2",
    "@types/usb": "^1.5.4",
    "@types/w3c-web-hid": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "alumina-visor": "^0.1.4",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "electron": "^16.0.6",
    "electron-builder": "^22.14.5",
    "electron-rebuild": "^3.2.5",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-standard": "^5.0.0",
    "estrella": "^1.4.1",
    "jest": "^27.4.7",
    "open": "^8.4.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.5.1",
    "reflect-metadata": "^0.1.13",
    "servor": "^4.0.2",
    "source-map-support": "^0.5.21",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "tar": "^6.1.9",
    "path-parse": "^1.0.7"
  },
  "volta": {
    "node": "14.18.2",
    "yarn": "1.22.17"
  }
}

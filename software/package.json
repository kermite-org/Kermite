{
  "name": "kermite",
  "productName": "Kermite",
  "version": "0.1.0-alpha.9",
  "main": "dist/shell/index.js",
  "author": "yahiro",
  "description": "keyboard software",
  "license": "MIT",
  "build": {
    "appId": "org.kermite.electron-app",
    "mac": {
      "target": "pkg",
      "icon": "misc/iconC.png"
    },
    "win": {
      "target": "nsis",
      "icon": "misc/iconC.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "files": [
      "dist/**/*"
    ],
    "directories": {
      "output": "release"
    },
    "buildDependenciesFromSource": true
  },
  "private": true,
  "scripts": {
    "postinstall": "electron-rebuild",
    "start": "ts-node build.ts -x-watch -x-exec",
    "start:dev": "cross-env NODE_ENV=development ts-node build.ts -x-watch -x-exec",
    "build": "cross-env NODE_ENV=production ts-node build.ts -x-build",
    "packaging:mac": "electron-builder --mac --x64",
    "packaging:win": "electron-builder --win --x64",
    "mockview": "cross-env NODE_ENV=development ts-node build2.ts -x-mockview",
    "web": "cross-env NODE_ENV=development ts-node build2.ts -x-web",
    "clean": "rm -rf dist .cache",
    "typecheck": "tsc",
    "typecheck:full": "tsc -b src/*",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "incver": "npm version patch"
  },
  "dependencies": {
    "@types/w3c-web-hid": "^1.0.2",
    "fast-equals": "^2.0.3",
    "glob": "^7.1.7",
    "immer": "^9.0.5",
    "node-fetch": "^2.6.1",
    "node-hid": "^2.1.1",
    "serialport": "^9.2.0",
    "usb": "1.6.5"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/node-fetch": "^2.5.12",
    "@types/node-hid": "^1.3.1",
    "@types/serialport": "^8.0.2",
    "@types/usb": "^1.5.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "child_process": "^1.0.2",
    "cross-env": "^7.0.3",
    "electron": "11.4.8",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^5.0.0",
    "estrella": "^1.4.1",
    "open": "^8.2.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.3.2",
    "reflect-metadata": "^0.1.13",
    "servor": "^4.0.2",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "tar": "^6.1.2",
    "path-parse": "^1.0.7"
  }
}
